        /* Reset some default styles */
        body, h1, p, img, a {
            margin: 0;
            padding: 0;
            text-decoration: none; /* Remove default underline for links */
            color: inherit; /* Inherit text color */
        }

        /* Style for each box */
        .box {
            width: 200px; /* Fixed width */
            height: 210px; /* Fixed height */
            margin: 20px;
            text-align: center;
            display: inline-block;
            border-radius: 8px;
            overflow: hidden;
            transition: transform 0.3s ease-in-out;
            box-shadow: 5px 5px 20px black;
        }

            /* Style for the image */
            .box img {
                height: 100%;
                object-fit: fill;
                /* Maintain aspect ratio and cover the box */
            }

            /* Hover effect */
            .box:hover {
                transform: scale(1.05);
            }

            /* Style for the text */
            .box a {
                display: flex;
                flex-direction: column;
                height: 100%;
                justify-content: flex-end;
            }

            .box p {
                background-color: rgba(255, 255, 255, 0.8); /* Semi-transparent white background for better text visibility */
                padding: 10px;
                font-size: 14px;
                box-sizing: border-box; /* Include padding in the total height */
            }
    </style>
